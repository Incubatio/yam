{
    "name": "incubatio/yam",
    "type": "application",
    "keywords": ["migration"],
    "description": "Yet Another Migrator: Flexible and customizable command line tool for migration with a trivial version management",
    "homepage": "http://github.com/Incubatio/yam",
    "license": "GPLv3.0",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Nicolas Morel",
            "email": "work@incubatio.org"
        }
    ],
    "config": {
        "vendor-dir":"vendor",
        "bin-dir":"bin"
    },
    "require": {
        "php": ">=5.3.0",
        "incube/base": "dev-master",
        "doctrine/common":"2.3.*@stable",
        "doctrine/mongodb-odm": "1.0.0-BETA7",
        "doctrine/orm": "2.3.*@stable"
    },
    "autoload": {
        "classmap": [
            "app/Services/Listeners"
        ]
    },
    "bin": ["bin/yam","bin/yam.php"]
}
